{% extends 'base.html.twig' %}

{% block title %}Quantareport{% endblock %}

{% block body %}
    <h1>Quantareport</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ quantareport.id }}</td>
            </tr>
            <tr>
                <th>Consult_method</th>
                <td>{{ quantareport.consultMethod }}</td>
            </tr>
            <tr>
                <th>Started_at</th>
                <td>{{ quantareport.startedAt ? quantareport.startedAt|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
            <tr>
                <th>Ended_at</th>
                <td>{{ quantareport.endedAt ? quantareport.endedAt|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
            <tr>
                <th>Soundplay</th>
                <td>{{ quantareport.soundplay }}</td>
            </tr>
            <tr>
                <th>Comment</th>
                <td>{{ quantareport.comment }}</td>
            </tr>
            <tr>
                <th>Rdate</th>
                <td>{{ quantareport.rdate ? quantareport.rdate|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_quantareport_index') }}">back to list</a>

    <a href="{{ path('app_quantareport_edit', {'id': quantareport.id}) }}">edit</a>

    {{ include('quantareport/_delete_form.html.twig') }}
{% endblock %}
