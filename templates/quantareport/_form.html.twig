<div class="row">
    <div class="">
        
        <div class="field_form form_style1 icon_form">
            {{ form_start(form) }}
                <div class="row">
                    <div class="form-group">
                        {{ form_label(form.consult_method) }}
                    
                        {{ form_widget(form.consult_method,{'attr':{'class':''}}) }}
                    
                    </div>
        
                    <div class="form-group ">
                        {{ form_label(form.user) }}
                        {{ form_widget(form.user,{'attr':{'class':''}}) }}
                        
                    
                    </div>
                    <div class="form-group ">
                        {{ form_label(form.client) }}
                        {{ form_widget(form.client,{'attr':{'class':''}}) }}
                        
                        <div class="class text-right " >
                            <a href="{{path('app_client_new')}}" class="btn btn-secondary" style="border-radius:50px; margin-bottom:50px;"> <i class=" ion-plus-circled"></i>Ajouter un client</a>
                
                        </div>
                    </div>
                    <div class="row " style="justify-content:center; display:flex;">
                            {{ form_label(form.pobject) }}
                        <div class="row" style="border-radius:0.375rem; background-color:#e9ecef; border: 1 solid; padding:0.375rem 0.75rem;">
                            
                            
                            <div class="col-8">
                            
                                {{ form_widget(form.pobject,{'attr':{'class':'form-check','required':'required', 'id':''}}) }}
                            </div>
                            
                            
                        
                            <div class="col-2" style="margin:auto; justify-content:center;">
                                <a href="{{path('app_primary_object_new')}}" class="btn" style="color:blue; font-size:1.5rem; "> nouveau motif</a>
                            </div>
                        </div>
                    </div>
                    <div class="row " style="justify-content:center; display:flex;  margin-bottom:75px;">
                            {{ form_label(form.sobject) }}
                        <div class="row" style="border-radius:0.375rem; background-color:#e9ecef; border: 1 solid; padding:0.375rem 0.75rem;">
                            
                            <div class="col-8">
                            
                                {{ form_widget(form.sobject,{'attr':{'class':'form-check','required':'required', 'id':''}}) }}
                            </div>
                            
                            <div class="col-2" style="margin:auto; justify-content:center;">
                            
                                <a href="{{path('app_secondary_object_new')}}" class="btn" style="color:blue; font-size:1.5rem; "> <i class=" ion-plus-circled"></i>nouveau motif</a>
                        
                            </div>
                        </div>
                    </div>
                
                          
                    {{ form_widget(form.gstate,{'attr':{'class':'', 'placeholder':" *",'required':'required', 'id':''}}) }}
                    
                
                    {{ form_widget(form.health,{'attr':{'class':'', 'placeholder':" *",'required':'required', 'id':''}}) }}

                    <div class="row h2 " style="justify-content:center; margin:35px 15px;">
                            Date de la séance
                    </div>
                    <div class="row" style="border-radius:1.5rem; background-color:#e9ecef; border: 2px solid blue; padding:0.375rem 0.75rem;">
                            

                    <div class="col-12">
                            <div class="row">
                                <div class="col-4">
                                    
                                    {{form_label(form.started_at)}}
                                </div>
                                <div class="col-8">
                                    {{form_widget(form.started_at)}}
                                </div>
                            </div>
                        
                        </div>
                    <div class="col-12">
                            <div class="row">
                                <div class="col-4">
                                    
                                    {{form_label(form.ended_at)}}
                                </div>
                                <div class="col-8">
                                    {{form_widget(form.ended_at)}}
                                </div>
                            </div>
                        
                        </div>
                        
                    </div>
                    <div style=" margin-bottom:75px;">
                    </div>
                    <div class="row h2 " style="justify-content:center; margin:35px 15px;">
                            Listes des musiques écoutées durant la séance
                    </div>
                    {{ form_widget(form.soundplay,{'attr':{'class':'form-control', 'placeholder':" Liste des musiques*",'required':'required', 'id':''}}) }}
                    
                    
                    <div style=" margin-bottom:75px;">
                     </div>
                
                    {{ form_widget(form.qdata,{'attr':{'class':'', 'placeholder':" *",'required':'required', 'id':''}}) }}
                    
                
                    <div class="row h2 " style="justify-content:center; margin:35px 15px;">
                            Observations
                    </div>
                    {{ form_widget(form.comment,{'attr':{'class':'form-control', 'placeholder':" *",'required':'required', 'id':''}}) }}
                    
                    <div class="row h2 " style="justify-content:center; margin:35px 15px;">
                            Prochain rendez-vous
                    </div> 
                
                    {{ form_widget(form.rdate,{'attr':{'class':'', 'placeholder':" *",'required':'required', 'id':''}}) }}
                    
                
                
                </div>
                <div class="row col-2"  style="justify-content:center; margin:auto; ">
                 <button class="btn btn-primary" style="margin:25px; " >{{ button_label|default('Sauvegarder') }}</button>
                </div>

                
                
            {{ form_end(form) }}
        </div>
    
    </div>
</div>