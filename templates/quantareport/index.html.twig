{% extends 'base.html.twig' %}

{% block title %}Quantareport index{% endblock %}

{% block body %}
    <h1>Quantareport index</h1>

    <table class="table">
        <thead>
            <tr>
                <th>Id</th>
                <th>Consult_method</th>
                <th>Started_at</th>
                <th>Ended_at</th>
                <th>Soundplay</th>
                <th>Comment</th>
                <th>Rdate</th>
                <th>actions</th>
            </tr>
        </thead>
        <tbody>
        {% for quantareport in quantareports %}
            <tr>
                <td>{{ quantareport.id }}</td>
                <td>{{ quantareport.consultMethod }}</td>
                <td>{{ quantareport.startedAt ? quantareport.startedAt|date('Y-m-d H:i:s') : '' }}</td>
                <td>{{ quantareport.endedAt ? quantareport.endedAt|date('Y-m-d H:i:s') : '' }}</td>
                <td>{{ quantareport.soundplay }}</td>
                <td>{{ quantareport.comment }}</td>
                <td>{{ quantareport.rdate ? quantareport.rdate|date('Y-m-d H:i:s') : '' }}</td>
                <td>
                    <a href="{{ path('app_quantareport_show', {'id': quantareport.id}) }}">show</a>
                    <a href="{{ path('app_quantareport_edit', {'id': quantareport.id}) }}">edit</a>
                </td>
            </tr>
        {% else %}
            <tr>
                <td colspan="8">no records found</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <a href="{{ path('app_quantareport_new') }}">Create new</a>
{% endblock %}
