{% extends 'base.html.twig' %}

{% block title %}ProfilCosmetique index{% endblock %}

{% block body %}
    <h1>ProfilCosmetique index</h1>

    <table class="table">
        <thead>
            <tr>
                <th>Id</th>
                <th>Nom</th>
                <th>actions</th>
            </tr>
        </thead>
        <tbody>
        {% for profil_cosmetique in profil_cosmetiques %}
            <tr>
                <td>{{ profil_cosmetique.id }}</td>
                <td>{{ profil_cosmetique.nom }}</td>
                <td>
                    <a href="{{ path('app_profil_cosmetique_show', {'id': profil_cosmetique.id}) }}">show</a>
                    <a href="{{ path('app_profil_cosmetique_edit', {'id': profil_cosmetique.id}) }}">edit</a>
                </td>
            </tr>
        {% else %}
            <tr>
                <td colspan="3">no records found</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <a href="{{ path('app_profil_cosmetique_new') }}">Create new</a>
{% endblock %}
