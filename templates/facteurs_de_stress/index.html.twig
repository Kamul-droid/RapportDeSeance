{% extends 'base.html.twig' %}

{% block title %}FacteursDeStress index{% endblock %}

{% block monTitre %}<div class="fst-italic"> Facteur De Stress  </div>{% endblock %}
{% block body %}
    <table class="table-sm table-responsive col-md-4 offset-4 table-bordered mg border border-5 border-info rounded-5">
        <thead>
            <tr>
                <th>Id</th>
                <th>Nom</th>
                <th>actions</th>
            </tr>
        </thead>
        <tbody>
        {% for facteurs_de_stress in facteurs_de_stresses %}
            <tr>
                <td>{{ facteurs_de_stress.id }}</td>
                <td>{{ facteurs_de_stress.nom }}</td>
                <td>
                    <a href="{{ path('app_facteurs_de_stress_show', {'id': facteurs_de_stress.id}) }}">show</a>
                    <a href="{{ path('app_facteurs_de_stress_edit', {'id': facteurs_de_stress.id}) }}">edit</a>
                </td>
            </tr>
        {% else %}
            <tr>
                <td colspan="3">no records found</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <div class="text-center"><a class=" btn btn-warning"  href="{{ path('app_facteurs_de_stress_new') }}">Create new</a></div>
{% endblock %}
