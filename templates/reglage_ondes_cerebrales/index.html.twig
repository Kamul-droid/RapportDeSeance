{% extends 'base.html.twig' %}

{% block title %}ReglageOndesCerebrales index{% endblock %}

{% block monTitre %}<div class="fst-italic"> Reglages Ondes Cerebrales </div>{% endblock %}

{% block body %}
    <table class="table-sm table-responsive col-md-4 offset-4 table-bordered mg border border-5 border-info rounded-5">
        <thead>
            <tr>
                <th>Id</th>
                <th>Nom</th>
                <th>actions</th>
            </tr>
        </thead>
        <tbody>
        {% for reglage_ondes_cerebrale in reglage_ondes_cerebrales %}
            <tr>
                <td>{{ reglage_ondes_cerebrale.id }}</td>
                <td>{{ reglage_ondes_cerebrale.nom }}</td>
                <td>
                    <a href="{{ path('app_reglage_ondes_cerebrales_show', {'id': reglage_ondes_cerebrale.id}) }}">show</a>
                    <a href="{{ path('app_reglage_ondes_cerebrales_edit', {'id': reglage_ondes_cerebrale.id}) }}">edit</a>
                </td>
            </tr>
        {% else %}
            <tr>
                <td colspan="3">no records found</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <div class="text-center"><a class=" btn btn-warning" href="{{ path('app_reglage_ondes_cerebrales_new') }}">Ajouter Reglages Ondes Cerebrales </a></div>
{% endblock %}
