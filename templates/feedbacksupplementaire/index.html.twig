{% extends 'base.html.twig' %}

{% block title %}Feedbacksupplementaire index{% endblock %}

{% block monTitre %} <div class="fst-italic">Ajout FeedBack Supplementaire </div>{% endblock %}
{% block body %}

    <table class="table-sm table-responsive col-md-4 offset-4 table-bordered mg border border-5 border-info rounded-5">
        <thead>
            <tr>
                <th>Id</th>
                <th>Nom</th>
                <th>actions</th>
            </tr>
        </thead>
        <tbody>
        {% for feedbacksupplementaire in feedbacksupplementaires %}
            <tr>
                <td>{{ feedbacksupplementaire.id }}</td>
                <td>{{ feedbacksupplementaire.nom }}</td>
                <td>
                    <a href="{{ path('app_feedbacksupplementaire_show', {'id': feedbacksupplementaire.id}) }}">show</a>
                    <a href="{{ path('app_feedbacksupplementaire_edit', {'id': feedbacksupplementaire.id}) }}">edit</a>
                </td>
            </tr>
        {% else %}
            <tr>
                <td colspan="3">no records found</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <div class="text-center"><a class=" btn btn-warning" href="{{ path('app_feedbacksupplementaire_new') }}">Create new</a></div>
{% endblock %}
