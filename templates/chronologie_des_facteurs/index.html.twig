{% extends 'base.html.twig' %}

{% block title %}Chronologie Des Facteurs{% endblock %}


{% block monTitre %}<div class="fst-italic"> Facteurs De Stress Relationnel </div>{% endblock %}
{% block body %}
    <table class="table-sm table-responsive col-md-4 offset-4 table-bordered mg border border-5 border-info rounded-5">
        <thead>
            <tr>
                <th>Id</th>
                <th>Nom</th>
                <th>actions</th>
            </tr>
        </thead>
        <tbody>
        {% for chronologie_des_facteur in chronologie_des_facteurs %}
            <tr>
                <td>{{ chronologie_des_facteur.id }}</td>
                <td>{{ chronologie_des_facteur.nom }}</td>
                <td>
                    <a href="{{ path('app_chronologie_des_facteurs_show', {'id': chronologie_des_facteur.id}) }}">show</a>
                    <a href="{{ path('app_chronologie_des_facteurs_edit', {'id': chronologie_des_facteur.id}) }}">edit</a>
                </td>
            </tr>
        {% else %}
            <tr>
                <td colspan="3">no records found</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>
    <div class="text-center"><a class=" btn btn-warning" href="{{ path('app_chronologie_des_facteurs_new') }}">Ajouter Choix Solution</a></div>
{% endblock %}
